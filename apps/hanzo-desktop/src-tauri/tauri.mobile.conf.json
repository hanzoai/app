{
  "bundle": {
    "active": true,
    "iOS": {
      "developmentTeam": ""
    }
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "url": "../index.html",
        "title": "Hanzo",
        "width": 375,
        "height": 812,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": {
        "default-src": "'self' customprotocol: asset:",
        "font-src": "'self' data: asset: https://fonts.gstatic.com",
        "img-src": "'self' asset: http://asset.localhost blob: data:",
        "style-src": "'unsafe-inline' 'self' https://fonts.googleapis.com"
      }
    }
  }
}
